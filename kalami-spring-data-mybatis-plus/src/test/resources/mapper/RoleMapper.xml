<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.apzda.kalami.mybatisplus.mapper.RoleMapper">
    <select id="getRoleById" resultType="com.apzda.kalami.mybatisplus.entity.Role">
        select *
        from t_roles
        where rid = #{id}
    </select>
</mapper>
